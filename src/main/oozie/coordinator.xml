<coordinator-app name="coord-${jobName}" frequency="${coord:minutes(coordTimeInterval)}"
	start="${coordStart}" end="${coordEnd}" timezone="${coordTimeZone}"
	xmlns="uri:oozie:coordinator:0.1">
	<controls>
		<timeout>${coordTimeOut}</timeout>
		<concurrency>${coordConcurrency}</concurrency>
		<execution>${coordExecution}</execution>
	</controls>
	<action>
		<workflow>
			<app-path>${jobFile}</app-path>
		</workflow>
	</action>
</coordinator-app>

